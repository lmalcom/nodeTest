{ 
  "name": "test ", 
  "classes": [ 
    "HTMLContainer",  
    "Navbar", 
    "DescriptionBox", 
    "Image", 
    "Text", 
    "Pagination", 
    "TextImageButton"
  ], 
  "content": { 
    "view": { 
      "blockClass": "Page", 
        "css":{ 
            "overflow-y":"auto", 
            "background":"url('images/bg/footer_lodyas.png')", 
            "& > *":{
                "margin-bottom":"10px", 
                "padding":"25px", 
                "box-shadow":"rgba(0,0,0,.5) 2px 2px 5px"
            }, 
            ".Navbar": {
                "padding":"0 25px"
            }, 
            "@mainColor":"#ffffff"
        }
    },
    "children": [ 
      {
          "blockClass":"Navbar", 
          "settings":{
              "css":{
                  "height":"60px", 
                  "color":"@mainColor"
              },
              "header":"Options", 
              "text":[
                  "Home", 
                  "About", 
                  "Contact", 
                  "Store"
              ]
          } 
      },
      { 
        "blockClass":"Pagination", 
        "settings":{ 
            "css":{ 
                "height":"e('calc(100% - 60px)')", 
                "& > .Button":{
                    "line-height":"60px", 
                    "background":"rgba(0,0,0,.7)", 
                    "font-size":"2em", 
                    "&:hover":{ 
                        "background":"rbga(0,0,0,.9)" 
                    } 
                }
            }, 
            "type":"TextImageButton", 
            "images":[
                "images/rootSmall.jpg", 
                "http://stuffpoint.com/food-network/image/174206-food-network-foodnetwork.jpg", 
                "http://3.bp.blogspot.com/-YogtSE2qnSw/TfNNUCSds-I/AAAAAAAAAJo/ofyYhir2nsI/s1600/food%2Bnetwork.jpg","http://img.foodnetwork.com/FOOD/2012/12/20/fnd_Hot-Peppers-2013-Trends_s4x3_lg.jpg", 
                "http://stuffpoint.com/food-network/image/174206-food-network-foodnetwork.jpg", 
                "http://3.bp.blogspot.com/-YogtSE2qnSw/TfNNUCSds-I/AAAAAAAAAJo/ofyYhir2nsI/s1600/food%2Bnetwork.jpg","http://img.foodnetwork.com/FOOD/2012/12/20/fnd_Hot-Peppers-2013-Trends_s4x3_lg.jpg", 
                "http://stuffpoint.com/food-network/image/174206-food-network-foodnetwork.jpg", 
                "http://3.bp.blogspot.com/-YogtSE2qnSw/TfNNUCSds-I/AAAAAAAAAJo/ofyYhir2nsI/s1600/food%2Bnetwork.jpg"
            ], 
            "text":[
                "om nom nom!!!", 
                "oh hey 2", 
                "oh hey 3", 
                "oh hey 4", 
                "oh hey 5", 
                "oh hey 6"
            ]
        } 
      }, 
      {
          "blockClass":"Text", 
          "settings":{
              "text":"New Section", 
              "type":"h2", 
              "css":{
                  "height":"60px", 
                  "background-color":"#1c1c1c",
                  "margin":0, 
                  "*":{
                      "margin":0
                  }
              }
          }
      },
      {
          "blockClass":"HTMLContainer", 
          "settings":{
              "css":{
                  "padding":"10px",
                  "height":"150px", 
                  "background-color":"#655e5a",
                  ".DescriptionBox":{
                      "height":"100%", 
                      "width":"calc(100% / 6)", 
                      "position":"relative", 
                      "float":"left", 
                      "color":"darken(@mainColor, 30%)",
                      "padding":"5px",
                      "box-shadow":"rgba(0,0,0,.7) 2px 2px 2px",
                      ".Image":{
                          "width":"100%", 
                          "max-height":"85%"
                      }, 
                      ".Text":{
                          "width":"100%",
                          "height":"15%",
                          "text-align":"center", 
                          "p":{
                              "margin":"0"
                          }
                      }, 
                      "&:hover":{
                          "background":"rgba(0,0,0,.9)", 
                          "cursor":"pointer"
                      }
                  }
              }  
          },
          "children":[
            {
                "DescriptionBox":{
                    "settings":{
                          "text":["Video", "Image", "ImageButton", "Text", "HTMLBlock", "Image"]
                      }
                }
            }
          ]
      },
      {
          "blockClass":"Text", 
          "settings":{
              "css":{
                  "height":"60px", 
                  "background-color":"#1c1c1c", 
                  "color":"@mainColor",
                  "h2":{
                      "margin":0
                  }
              }, 
              "type":"h2", 
          "text":"Header 1" 
          }
      }, 
        {
          "blockClass":"HTMLContainer", 
          "settings":{
              "css":{
                  "padding":"10px", 
                  "background-color":"#655e5a",
                  ".DescriptionBox":{
                      "height":"150px", 
                      "width":"e('calc(50% - 5px)')", 
                      "position":"relative", 
                      "float":"left", 
                      "color":"darken(@mainColor, 30%)",
                      "padding":"10px",
                      "margin":"5px 5px 0 0", 
                      "box-shadow":"rgba(0,0,0,.7) 2px 2px 2px",
                      "background-color":"#1c1c1c",
                      ".Image":{
                          
                      }, 
                      ".Text":{
                          
                      }, 
                      "&:hover":{
                          "background":"rgba(0,0,0,.9)", 
                          "cursor":"pointer"
                      }
                  }
              }  
          },
          "children":[
            {
                "DescriptionBox":{
                    "settings":{
                          "text":["Video", "Image", "ImageButton", "Text", "HTMLBlock", "Image"]
                      }
                }
            }
          ]
      }
    ] 
  } 
} 