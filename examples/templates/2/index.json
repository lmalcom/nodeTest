{ 
  "name": "test ", 
  "classes": [ 
    "HTMLContainer",  
    "Navbar", 
    "TextImageButton", 
    "GridContainer"
  ], 
  "content": { 
      "blockClass": "Page", 
      "settings":{
        "css":{ 
            "overflow-y":"auto", 
            "background":"rgb(25, 25, 25)", 
            ".Navbar": {
                "position":"absolute", 
                "bottom":"0", 
                "left":"0",
                "padding":"0 25px", 
                "z-index":"2"
            }, 
            "@mainColor":"#ffffff", 
            "@bg":"rgb(25,25,25)"
        }
    },
    "children": [ 
      {
          "blockClass":"Navbar", 
          "settings":{
              "css":{
                  "height":"60px", 
                  "background-color":"lighten(@bg, 25%)"
              },
              "header":"Options", 
              "text":[
                  "Home", 
                  "About", 
                  "Contact", 
                  "Store"
              ]
          }
      },
      {
          "blockClass":"GridContainer", 
          "settings":{
              "css":{
                  "padding":0, 
                  "position":"fixed", 
                  "overflow-y":"auto", 
                  ".TextImageButton":{
                      "background-color":"lighten(@bg, 40%)", 
                      "position":"relative", 
                      "float":"left", 
                      "height":"150px", 
                      "color":"darken(@mainColor, 30%)",
                      ".Image":{
                          "width":"100%", 
                          "position":"absolute", 
                          "z-index":-1
                      }, 
                      ".Text":{
                          "text-align":"center", 
                          "width":"100%", 
                          "position":"absolute", 
                          "z-index":1
                      }
                  }
              }  
          },
          "children":[
             { 
                  "TextImageButton":{
                      "settings":{
                          "css":{
                              ".Button":{
                                  "background-color":"rgba(red(@bg), green(@bg), blue(@bg), .7)"  
                              }
                          },
                          "text":[
